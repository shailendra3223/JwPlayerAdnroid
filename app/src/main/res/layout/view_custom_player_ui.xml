        <ImageView
            android:id="@+id/tv_fast_backward_15"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/ic_replay"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/play_pause_toggle"
            app:layout_constraintHorizontal_bias="0.45"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/play_pause_toggle"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/ic_jw_play"
            android:visibility="visible"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/tv_fast_forward_15"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/ic_forward"
            android:visibility="gone"
            app:layout_constraintHorizontal_bias="0.55"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/play_pause_toggle"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            tools:text="@string/subtitles_amp_audio"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:textStyle="bold"
            android:layout_margin="10dp"
            app:drawableStartCompat="@drawable/ic_jw_close"
            app:drawableTint="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:visibility="gone"
            tools:visibility="visible"/>

        <ImageView
            android:id="@+id/iv_zoom_in_out"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="10dp"
            android:contentDescription="@string/jwplayer_chromecast"
            android:padding="10dp"
            android:src="@drawable/ic_full_screen"
            app:layout_constraintBottom_toBottomOf="@+id/tv_title"
            app:layout_constraintEnd_toStartOf="@+id/iv_chrome_cast"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_title"
            android:visibility="gone"
            tools:visibility="visible"/>

<!--        <ImageView-->
<!--            android:id="@+id/iv_chrome_cast"-->
<!--            android:layout_width="50dp"-->
<!--            android:layout_height="50dp"-->
<!--            android:layout_marginVertical="10dp"-->
<!--            android:contentDescription="@string/jwplayer_chromecast"-->
<!--            android:src="@drawable/ic_jw_cast_off"-->
