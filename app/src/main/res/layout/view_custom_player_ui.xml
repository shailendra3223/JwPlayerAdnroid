<!--            app:layout_constraintEnd_toEndOf="@+id/seekbar"-->
<!--            app:layout_constraintRight_toRightOf="parent"-->
<!--            android:contentDescription="@string/jwplayer_exit_fullscreen"-->
<!--            android:visibility="gone"-->
<!--            tools:visibility="visible"/>-->

        <SeekBar
            android:id="@+id/seekbar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="2dp"
            android:layout_marginTop="100dp"
            android:progressBackgroundTint="@color/white"
            android:progressTint="@color/red"
            android:thumbTint="@color/white"
            android:visibility="gone"
            app:layout_constraintEnd_toStartOf="@+id/tv_time"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/play_pause_toggle"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            tools:text="00:00:00"
            android:textColor="@color/white"
            android:textSize="11sp"
            android:layout_marginEnd="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/seekbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/seekbar"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_video_setting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="4dp"
            android:layout_marginTop="8dp"
            android:drawablePadding="8dp"
            android:gravity="center_vertical"
            android:padding="12dp"
            android:text="@string/video_setting"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="gone"
            app:drawableStartCompat="@drawable/ic_video_setting"
            app:drawableTint="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/tv_episode"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_subtitle"
            app:layout_constraintTop_toBottomOf="@+id/seekbar"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_subtitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="8dp"
            android:gravity="center_vertical"
            android:text="@string/jwplayer_subtitles"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="gone"
            app:drawableStartCompat="@drawable/ic_subtitles"
            app:drawableTint="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/tv_video_setting"
            app:layout_constraintEnd_toStartOf="@+id/tv_video_setting"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_video_setting"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_episode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="8dp"
            android:gravity="center_vertical"
            android:text="@string/episodes"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="gone"
            app:drawableStartCompat="@drawable/ic_episode"
            app:drawableTint="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/tv_video_setting"
            app:layout_constraintEnd_toStartOf="@+id/tv_next_episode"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_video_setting"
            app:layout_constraintTop_toTopOf="@+id/tv_video_setting"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_next_episode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="4dp"
            android:gravity="center_vertical"
            android:text="@string/jwplayer_next_up"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="gone"
            app:drawableStartCompat="@drawable/ic_next_episode"
            app:drawableTint="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/tv_video_setting"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_episode"
            app:layout_constraintTop_toTopOf="@+id/tv_video_setting"
            tools:visibility="visible" />

</merge>

