        <!--            tools:visibility="visible" />        -->

        <ImageView
            android:id="@+id/iv_chrome_cast"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginVertical="10dp"
            android:contentDescription="@string/jwplayer_chromecast"
            android:src="@drawable/ic_jw_cast_off"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/tv_title"
            app:layout_constraintEnd_toEndOf="@+id/tv_time"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_title"
            tools:visibility="gone" />

        <androidx.mediarouter.app.MediaRouteButton
            android:id="@+id/media_route_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:mediaRouteTypes="user"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="@+id/iv_chrome_cast"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_chrome_cast"
            tools:visibility="gone" />

